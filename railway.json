{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "server/Dockerfile"
  },
  "deploy": {
    "startCommand": "node /directus/cli.js bootstrap && node /directus/cli.js start",
    "healthcheckPath": "/server/health",
    "healthcheckTimeout": 300
  }
}
